{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
        "no-console": 0,
        "quotes": ["warn", "double"],
        "indent": ["warn", 4],
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0
    }
}
